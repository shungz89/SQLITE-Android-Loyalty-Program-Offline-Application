<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="my.com.durianz"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="15"
        android:targetSdkVersion="27" />

    <application
        android:allowBackup="true"
        android:debuggable="true"
        android:icon="@drawable/durianz"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="my.com.durianz.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="my.com.durianz.Transaction" />
        <activity android:name="my.com.durianz.CreateAccount" />
        <activity android:name="my.com.durianz.EditAccount" />
    </application>

</manifest>